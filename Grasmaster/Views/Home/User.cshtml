@*
    
*@
@using Grasmaster.Infrastructure.Models
@model IEnumerable<User>

@{
    ViewData["Title"] = "Product";
}

<table cellpadding="0" cellspacing="0">
    <tr>
        <th>Name</th>
        <th>Description</th>
    </tr>
    @foreach (User user in Model ?? throw new ArgumentNullException())
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.Name</td>
            <td>@user.Address</td>
        </tr>
    }
</table>
