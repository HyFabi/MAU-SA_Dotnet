@*
	
*@
@using At.Mausa.Grasmaster.Domain.Models.Domain
@model IEnumerable<User>

@{
	ViewData["Title"] = "Product";
}

<table cellpadding="0" cellspacing="0">
	<tr>
		<th>ID</th>
		<th>Name</th>
		<th>Address</th>
	</tr>
	@foreach (User user in Model ?? throw new ArgumentNullException())
	{
		<tr>
			<td>@user.Id</td>
			<td>@user.Name</td>
			<td>@user.Address</td>
		</tr>
	}
</table>
